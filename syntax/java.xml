<xml>
    <!--Highlighting RegEx-->
    <syntax type="numberLiteral">([0-9]+\.?[0-9]*f?)</syntax>
    <syntax type="alphabetical">([a-zA-Z]+[0-9]*[a-zA-Z]*)</syntax>
    <syntax type="break">( |;|,)</syntax>
    <syntax type="grouping">(\(|\)|\{|\}|\[|\])</syntax>
    <syntax type="stringLiteral">&quot;.*&quot;</syntax>
    <syntax type="characterLiteral">&apos;.?&apos;</syntax>
    <syntax type="comment">(\/\/.*)</syntax>
    <syntax type="operator">(\+|\-|\*|((?&lt;!\/)\/(?!\/))|\=|\%|\&gt;|\&lt;|\||\&amp;|\!)</syntax>
    <!--Keywords-->
    <keywords>
        <keyword>abstract</keyword>
