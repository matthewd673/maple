<Properties>
    <DebugTokens>false</DebugTokens>
    <NoHighlight>false</NoHighlight>
    <NoTokenize>false</NoTokenize>
    <CliNoHighlight>false</CliNoHighlight>
    <RelativePath>false</RelativePath>
    <NavigatePastTabs>true</NavigatePastTabs>
    <DeleteEntireTabs>true</DeleteEntireTabs>
    <ReadOnly>false</ReadOnly>
    <EnableLogging>true</EnableLogging>
    <SummarizeLog>false</SummarizeLog>
    <SaveOnClose>false</SaveOnClose>
    <PreserveIndentOnEnter>true</PreserveIndentOnEnter>
    <ShiftSelect>true</ShiftSelect>
    <ShiftDeindent>true</ShiftDeindent>
    <ArrowsDeselect>true</ArrowsDeselect>
    <ClearOutputOnToggle>false</ClearOutputOnToggle>
    <ColorOutputBackground>true</ColorOutputBackground>
    <HighlightTrailingWhitespace>true</HighlightTrailingWhitespace>
    <Autocomplete>true</Autocomplete>
    <AutocompleteOnlyAtEOL>false</AutocompleteOnlyAtEOL>
    <AutoResize>true</AutoResize>
    <ExternalEditAlert>true</ExternalEditAlert>
    <HideGutter>false</HideGutter>
    <ColorGutterBackground>false</ColorGutterBackground>

    <ThemeDirectory>{mapledir}\themes\</ThemeDirectory>
    <ThemeFile>maple.xml</ThemeFile>
    <SyntaxDirectory>{mapledir}\syntax\</SyntaxDirectory>
    <FooterLayoutFile>{mapledir}\properties\footer.xml</FooterLayoutFile>
    <TabSpacesCount>4</TabSpacesCount>
    <ScrollYIncrement>half</ScrollYIncrement>
    <ScrollXIncrement>half</ScrollXIncrement>
    <DefaultEncoding>utf8</DefaultEncoding>
    <HistoryMaxSize>5000</HistoryMaxSize>

    <GutterLeftPad>0</GutterLeftPad>
    <GutterBarrier> </GutterBarrier>
    <GutterPadding>1</GutterPadding>
    <OverflowIndicator>…</OverflowIndicator>

    <Aliases>
        <Alias command="help">h</Alias>
        <Alias command="help">?</Alias>
        <Alias command="save">s</Alias>
        <Alias command="load">l</Alias>
        <Alias command="load">open</Alias>
        <Alias command="top">t</Alias>
        <Alias command="bot">b</Alias>
        <Alias command="bot">bottom</Alias>
        <Alias command="selectin">i</Alias>
        <Alias command="selectout">o</Alias>
        <Alias command="deselect">d</Alias>
        <Alias command="readonly">ro</Alias>
        <Alias command="goto">go</Alias>
        <Alias command="goto">g</Alias>
        <Alias command="goto">line</Alias>
        <Alias command="close">quit</Alias>
        <Alias command="find">f</Alias>
        <Alias command="deindent">de</Alias>
        <Alias command="count">ct</Alias>
    </Aliases>

    <Shortcuts>
        <Shortcut key="s" execute="true">save</Shortcut>
        <Shortcut key="l" execute="false">load </Shortcut>
        <Shortcut key="f" execute="false">find </Shortcut>
        <Shortcut key="c" execute="true">copy</Shortcut>
        <Shortcut key="p" execute="true">paste</Shortcut>
        <Shortcut key="x" execute="true">cut</Shortcut>
        <Shortcut key="q" execute="true">close</Shortcut>
        <Shortcut key="z" execute="true">undo</Shortcut>
        <Shortcut key="y" execute="true">redo</Shortcut>
        <Shortcut key="a" execute="true">selectall</Shortcut>
        <Shortcut key="/" execute="true">comment</Shortcut>
    </Shortcuts>
</Properties>