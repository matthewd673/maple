<Properties>
    <DebugTokens>false</DebugTokens>
    <NoHighlight>false</NoHighlight>
    <NoTokenize>false</NoTokenize>
    <CliNoHighlight>false</CliNoHighlight>
    <RelativePath>false</RelativePath>
    <NavigatePastTabs>true</NavigatePastTabs>
    <DeleteEntireTabs>true</DeleteEntireTabs>
    <ReadOnly>false</ReadOnly>
    <EnableLogging>true</EnableLogging>
    <SummarizeLog>false</SummarizeLog>
    <SaveOnClose>false</SaveOnClose>
    <PreserveIndentOnEnter>true</PreserveIndentOnEnter>
    <ShiftSelect>true</ShiftSelect>
    <ShiftDeindent>true</ShiftDeindent>
    <ArrowsDeselect>true</ArrowsDeselect>
    <ClearOutputOnToggle>false</ClearOutputOnToggle>
    <ColorOutputBackground>true</ColorOutputBackground>
    <HighlightTrailingWhitespace>false</HighlightTrailingWhitespace>
    <Autocomplete>true</Autocomplete>
    <AutoResize>true</AutoResize>
    
    <ThemeDirectory>{mapledir}\themes\</ThemeDirectory>
    <ThemeFile>maple.xml</ThemeFile>
    <SyntaxDirectory>{mapledir}\syntax\</SyntaxDirectory>
    <TabSpacesCount>4</TabSpacesCount>
    <ScrollYIncrement>half</ScrollYIncrement>
    <ScrollXIncrement>half</ScrollXIncrement>
    <DefaultEncoding>utf8</DefaultEncoding>
    <HistoryMaxSize>5000</HistoryMaxSize>

    <GutterLeftPad>0</GutterLeftPad>
    <GutterBarrier> </GutterBarrier>
    <OverflowIndicator>…</OverflowIndicator>
</Properties>