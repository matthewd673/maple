<Theme>
    <Text>Gray</Text>
    <Accent>Cyan</Accent>
    <Error>Red</Error>
    <Gutter>DarkGray</Gutter>
    <Selection>Blue</Selection>
    
    <CliInputDefault>Cyan</CliInputDefault>
    <CliOutputInfo>Yellow</CliOutputInfo>
    <CliOutputError>Red</CliOutputError>
    <CliOutputSuccess>Green</CliOutputSuccess>
    <CliPrompt>Cyan</CliPrompt>

    <TokenColors>
        <Token type="NumberLiteral">Red</Token>
        <Token type="StringLiteral">Green</Token>
        <Token type="CharacterLiteral">Green</Token>
        <Token type="BooleanLiteral">Yellow</Token>
        <Token type="HexLiteral">Red</Token>
        <Token type="Variable">Gray</Token>
        <Token type="Keyword">Cyan</Token>
        <Token type="Comment">DarkGray</Token>
        <Token type="Grouping">Gray</Token>
        <Token type="Operator">Magenta</Token>
        <Token type="Function">Blue</Token>
        <Token type="Url">Yellow</Token>
        <Token type="SpecialChar">Magenta</Token>
        <Token type="TrailingWhitespace">DarkGray</Token>

        <Token type="CliCommandValid">Cyan</Token>
        <Token type="CliCommandInvalid">Red</Token>
        <Token type="CliSwitch">DarkGray</Token>
        <Token type="CliString">Green</Token>
    </TokenColors>
</Theme>
