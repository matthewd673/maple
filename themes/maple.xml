<xml>
    <!--Maple Colors-->
    <color category="text">Gray</color>
    <color category="accent">Yellow</color>
    <color category="error">Red</color>
    <color category="gutter">DarkGray</color>
    <color category="selection">Blue</color>
    <!--Syntax Colors-->
    <color category="numberLiteral">Magenta</color>
    <color category="stringLiteral">Green</color>
    <color category="characterLiteral">Red</color>
    <color category="booleanLiteral">Blue</color>
    <color category="variable">Gray</color>
    <color category="keyword">Yellow</color>
    <color category="comment">DarkGray</color>
    <color category="grouping">Gray</color>
    <color category="operator">Red</color>
    <!--CLI Colors-->
    <color category="cliInputDefault">Yellow</color>
    <color category="cliOutputInfo">Cyan</color>
    <color category="cliOutputError">Red</color>
    <color category="cliOutputSuccess">Green</color>
    <color category="cliPrompt">Yellow</color>
    <color category="cliCommandValid">Yellow</color>
    <color category="cliCommandInvalid">Red</color>
    <color category="cliSwitch">DarkGray</color>
    <color category="cliString">Green</color>
    <!--Editor Customizations-->
    <string category="vanityFooter">maple</string>
    <string category="gutterLeftPad">0</string>
</xml>
